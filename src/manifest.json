{
    "name": "LC NoWidth",
    "version": "0.0.2",
    "manifest_version": 3,
    "description": "Adjusts the min-width on LeetCode website to make the small-width window display correctly.",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/*"
            ],
            "js": [
                "./content_scripts/contentScript.js"
            ]
        }
    ],
    "permissions": [],
    "action": {}
}